    "def alliterationMaker(alist,char):\n",
    "    for i in alist:\n",
    "        if i[0] != char:\n",
    "            alist.pop(alist.index(i))\n",
    "    alist.sort()\n",
    "    return alist\n",
    "if __name__ == '__main__':\n",
    "    print(alliterationMaker([\"big\",\"cow\",\"bug\",\"bit\",\"table\",\"black\",\"bear\"],\"b\"))\n"
   ]
